      <Script># Copyright(c) 2017, Dimitar Venkov
# @5devene, dimitar.ven@gmail.com
# www.badmonkeys.net

def tolist(x):
	if hasattr(x,'__iter__'): return x
	else : return [x]

def n2s(n, digits=IN[1]):
	if digits is not None:
		n = round(n, digits)
	s1 = str(n)
	if s1[-2:] == ".0":
		s1 = s1[:-2]
	return s1

nums = tolist(IN[0])

OUT = map(n2s, nums)</Script>
